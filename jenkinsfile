pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
           echo "build"
           sh "javac HelloWorld.java"
            }
        }
        stage('Execute') {
            steps {
               echo "Execute"
               sh "java HelloWorld.java"
            }
        }
        stage('Deploy') {
            steps {
              echo "deploy"
            }
        }
    }
}
